{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[a-z0-9_-]+$"
    },
    "linkage": {
      "type": "enumeration",
      "enum": [
        "navbar",
        "footer"
      ],
      "default": "navbar",
      "required": true
    },
    "linkage_position": {
      "type": "string",
      "required": true,
      "default": "navbar_099",
      "regex": "^(navbar_(0\\d{2})|footer_(1\\d{2}))$",
      "unique": true
    }
  }
}
